{
  "name": "es6-grunt-starter-kit",
  "version": "1.0.0",
  "description": "Babel + Grunt + Karma + ESLint + ESDoc starter kit",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack src/Foo.js dist/Foo.js --config tasks/webpack.config.dev.js --progress --colors --watch",
    "dist": "npm run lint && webpack src/Foo.js dist/Foo.min.js --config tasks/webpack.config.prod.js",
    "lint": "eslint src/**/*.js",
    "docs": "esdoc -c esdoc.conf.json",
    "karma:tdd": "karma start karma/dev.conf.js",
    "karma:ci": "karma start karma/ci.conf.js"
  },
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "karma",
    "eslint",
    "esdoc",
    "starter",
    "kit"
  ],
  "author": "Anzor Bashkhaz",
  "license": "ISC",
  "devDependencies": {
    "babel-istanbul": "^0.7.0",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.12.3",
    "browser-sync-webpack-plugin": "^1.0.1",
    "es6-shim": "^0.35.0",
    "esdoc": "^0.4.6",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-es6-shim": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.11.0",
    "phantomjs-prebuilt": "^2.1.7",
    "webpack": "^1.12.15"
  }
}
